resource "google_backup_dr_backup_vault" "name" {
  backup_minimum_enforced_retention_duration = string (Required)
  backup_vault_id = string (Required)
  location = string (Required)
  access_restriction = string (Optional)
  allow_missing = bool (Optional)
  annotations = ['map', 'string'] (Optional)
  backup_count = string (Computed)
  backup_retention_inheritance = string (Optional)
  create_time = string (Computed)
  deletable = bool (Computed)
  description = string (Optional)
  effective_annotations = ['map', 'string'] (Computed)
  effective_labels = ['map', 'string'] (Computed)
  effective_time = string (Optional)
  etag = string (Computed)
  force_delete = bool (Optional)
  force_update = bool (Optional)
  id = string (Optional, Computed)
  ignore_backup_plan_references = bool (Optional)
  ignore_inactive_datasources = bool (Optional)
  labels = ['map', 'string'] (Optional)
  name = string (Computed)
  project = string (Optional, Computed)
  service_account = string (Computed)
  state = string (Computed)
  terraform_labels = ['map', 'string'] (Computed)
  total_stored_bytes = string (Computed)
  uid = string (Computed)
  update_time = string (Computed)

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
