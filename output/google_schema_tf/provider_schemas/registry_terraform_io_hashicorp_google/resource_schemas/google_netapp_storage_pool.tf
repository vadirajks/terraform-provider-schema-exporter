resource "google_netapp_storage_pool" "name" {
  capacity_gib = string (Required)
  location = string (Required)
  name = string (Required)
  network = string (Required)
  service_level = string (Required)
  active_directory = string (Optional)
  allow_auto_tiering = bool (Optional)
  available_throughput_mibps = number (Computed)
  cold_tier_size_used_gib = string (Computed)
  custom_performance_enabled = bool (Optional)
  description = string (Optional)
  effective_labels = ['map', 'string'] (Computed)
  enable_hot_tier_auto_resize = bool (Optional)
  encryption_type = string (Computed)
  hot_tier_size_gib = string (Optional)
  hot_tier_size_used_gib = string (Computed)
  id = string (Optional, Computed)
  kms_config = string (Optional)
  labels = ['map', 'string'] (Optional)
  ldap_enabled = bool (Optional)
  project = string (Optional, Computed)
  qos_type = string (Optional, Computed)
  replica_zone = string (Optional)
  terraform_labels = ['map', 'string'] (Computed)
  total_iops = string (Optional, Computed)
  total_throughput_mibps = string (Optional, Computed)
  volume_capacity_gib = string (Computed)
  volume_count = number (Computed)
  zone = string (Optional, Computed)

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
