resource "google_compute_interconnect_attachment" "name" {
  name = string (Required)
  router = string (Required)
  admin_enabled = bool (Optional)
  attachment_group = string (Computed)
  bandwidth = string (Optional, Computed)
  candidate_subnets = ['list', 'string'] (Optional)
  cloud_router_ip_address = string (Computed)
  cloud_router_ipv6_address = string (Computed)
  creation_timestamp = string (Computed)
  customer_router_ip_address = string (Computed)
  customer_router_ipv6_address = string (Computed)
  description = string (Optional)
  edge_availability_domain = string (Optional, Computed)
  effective_labels = ['map', 'string'] (Computed)
  encryption = string (Optional)
  google_reference_id = string (Computed)
  id = string (Optional, Computed)
  interconnect = string (Optional)
  ipsec_internal_addresses = ['list', 'string'] (Optional)
  label_fingerprint = string (Computed)
  labels = ['map', 'string'] (Optional)
  mtu = string (Optional, Computed)
  pairing_key = string (Computed)
  partner_asn = string (Computed)
  private_interconnect_info = ['list', ['object', {'tag8021q': 'number'}]] (Computed)
  project = string (Optional, Computed)
  region = string (Optional, Computed)
  self_link = string (Computed)
  stack_type = string (Optional, Computed)
  state = string (Computed)
  subnet_length = number (Optional)
  terraform_labels = ['map', 'string'] (Computed)
  type = string (Optional, Computed)
  vlan_tag8021q = number (Optional, Computed)

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
