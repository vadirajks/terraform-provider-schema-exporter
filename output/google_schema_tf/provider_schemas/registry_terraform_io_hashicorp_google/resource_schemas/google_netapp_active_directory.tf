resource "google_netapp_active_directory" "name" {
  dns = string (Required)
  domain = string (Required)
  location = string (Required)
  name = string (Required)
  net_bios_prefix = string (Required)
  password = string (Required)
  username = string (Required)
  administrators = ['list', 'string'] (Optional)
  aes_encryption = bool (Optional)
  backup_operators = ['list', 'string'] (Optional)
  create_time = string (Computed)
  description = string (Optional)
  effective_labels = ['map', 'string'] (Computed)
  encrypt_dc_connections = bool (Optional)
  id = string (Optional, Computed)
  kdc_hostname = string (Optional)
  kdc_ip = string (Optional)
  labels = ['map', 'string'] (Optional)
  ldap_signing = bool (Optional)
  nfs_users_with_ldap = bool (Optional)
  organizational_unit = string (Optional, Computed)
  project = string (Optional, Computed)
  security_operators = ['list', 'string'] (Optional)
  site = string (Optional)
  state = string (Computed)
  state_details = string (Computed)
  terraform_labels = ['map', 'string'] (Computed)

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
