data "azurerm_kubernetes_cluster_node_pool" "name" {
  kubernetes_cluster_name = string (Required)
  name = string (Required)
  resource_group_name = string (Required)
  auto_scaling_enabled = bool (Computed)
  eviction_policy = string (Computed)
  gpu_driver = string (Computed)
  id = string (Optional, Computed)
  max_count = number (Computed)
  max_pods = number (Computed)
  min_count = number (Computed)
  mode = string (Computed)
  node_count = number (Computed)
  node_labels = ['map', 'string'] (Computed)
  node_public_ip_enabled = bool (Computed)
  node_public_ip_prefix_id = string (Computed)
  node_taints = ['list', 'string'] (Computed)
  orchestrator_version = string (Computed)
  os_disk_size_gb = number (Computed)
  os_disk_type = string (Computed)
  os_type = string (Computed)
  priority = string (Computed)
  proximity_placement_group_id = string (Computed)
  spot_max_price = number (Computed)
  tags = ['map', 'string'] (Computed)
  upgrade_settings = ['list', ['object', {'drain_timeout_in_minutes': 'number', 'max_surge': 'string', 'node_soak_duration_in_minutes': 'number'}]] (Computed)
  vm_size = string (Computed)
  vnet_subnet_id = string (Computed)
  zones = ['list', 'string'] (Computed)

  timeouts block "single" (Optional) {
    read = string (Optional)
  }
}
