data "aws_elasticache_replication_group" "name" {
  replication_group_id = string (Required)
  arn = string (Computed)
  auth_token_enabled = bool (Computed)
  automatic_failover_enabled = bool (Computed)
  cluster_mode = string (Computed)
  configuration_endpoint_address = string (Computed)
  description = string (Computed)
  id = string (Optional, Computed)
  log_delivery_configuration = ['set', ['object', {'destination': 'string', 'destination_type': 'string', 'log_format': 'string', 'log_type': 'string'}]] (Computed)
  member_clusters = ['set', 'string'] (Computed)
  multi_az_enabled = bool (Computed)
  node_type = string (Computed)
  num_cache_clusters = number (Computed)
  num_node_groups = number (Computed)
  port = number (Computed)
  primary_endpoint_address = string (Computed)
  reader_endpoint_address = string (Computed)
  region = string (Optional, Computed)
  replicas_per_node_group = number (Computed)
  snapshot_retention_limit = number (Computed)
  snapshot_window = string (Computed)
}
