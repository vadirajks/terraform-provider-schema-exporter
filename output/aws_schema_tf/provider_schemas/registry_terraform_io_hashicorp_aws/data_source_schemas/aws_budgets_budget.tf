  budget_exceeded = bool (Computed)
  budget_limit = ['list', ['object', {'amount': 'string', 'unit': 'string'}]] (Computed)
  budget_type = string (Computed)
  calculated_spend = ['list', ['object', {'actual_spend': ['list', ['object', {'amount': 'string', 'unit': 'string'}]]}]] (Computed)
  cost_filter = ['set', ['object', {'name': 'string', 'values': ['list', 'string']}]] (Computed)
  cost_types = ['list', ['object', {'include_credit': 'bool', 'include_discount': 'bool', 'include_other_subscription': 'bool', 'include_recurring': 'bool', 'include_refund': 'bool', 'include_subscription': 'bool', 'include_support': 'bool', 'include_tax': 'bool', 'include_upfront': 'bool', 'use_amortized': 'bool', 'use_blended': 'bool'}]] (Computed)
