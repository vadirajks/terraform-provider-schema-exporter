data "aws_lex_bot" "name" {
  name = string (Required)
  arn = string (Computed)
  checksum = string (Computed)
  child_directed = bool (Computed)
  created_date = string (Computed)
  description = string (Computed)
  detect_sentiment = bool (Computed)
  enable_model_improvements = bool (Computed)
  failure_reason = string (Computed)
  id = string (Optional, Computed)
  idle_session_ttl_in_seconds = number (Computed)
  last_updated_date = string (Computed)
  locale = string (Computed)
  nlu_intent_confidence_threshold = number (Computed)
  region = string (Optional, Computed)
  status = string (Computed)
  version = string (Optional)
  voice_id = string (Computed)
}
