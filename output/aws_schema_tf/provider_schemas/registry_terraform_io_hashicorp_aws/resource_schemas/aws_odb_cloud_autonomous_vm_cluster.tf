resource "aws_odb_cloud_autonomous_vm_cluster" "name" {
  autonomous_data_storage_size_in_tbs = number (Required)
  cloud_exadata_infrastructure_id = string (Required)
  cpu_core_count_per_node = number (Required)
  db_servers = ['set', 'string'] (Required)
  display_name = string (Required)
  memory_per_oracle_compute_unit_in_gbs = number (Required)
  odb_network_id = string (Required)
  scan_listener_port_non_tls = number (Required)
  scan_listener_port_tls = number (Required)
  total_container_databases = number (Required)
  arn = string (Computed)
  autonomous_data_storage_percentage = number (Computed)
  available_autonomous_data_storage_size_in_tbs = number (Computed)
  available_container_databases = number (Computed)
  available_cpus = number (Computed)
  compute_model = string (Computed)
  cpu_core_count = number (Computed)
  cpu_percentage = number (Computed)
  created_at = string (Computed)
  data_storage_size_in_gbs = number (Computed)
  data_storage_size_in_tbs = number (Computed)
  description = string (Optional)
  domain = string (Computed)
  exadata_storage_in_tbs_lowest_scaled_value = number (Computed)
  hostname = string (Computed)
  id = string (Computed)
  is_mtls_enabled_vm_cluster = bool (Optional, Computed)
  license_model = string (Optional, Computed)
  max_acds_lowest_scaled_value = number (Computed)
  memory_size_in_gbs = number (Computed)
  node_count = number (Computed)
  non_provisionable_autonomous_container_databases = number (Computed)
  oci_resource_anchor_name = string (Computed)
  oci_url = string (Computed)
  ocid = string (Computed)
  odb_node_storage_size_in_gbs = number (Computed)
  percent_progress = number (Computed)
  provisionable_autonomous_container_databases = number (Computed)
  provisioned_autonomous_container_databases = number (Computed)
  provisioned_cpus = number (Computed)
  reclaimable_cpus = number (Computed)
  region = string (Optional, Computed)
  reserved_cpus = number (Computed)
  shape = string (Computed)
  status = string (Computed)
  status_reason = string (Computed)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Computed)
  time_database_ssl_certificate_expires = string (Computed)
  time_ords_certificate_expires = string (Computed)
  time_zone = string (Optional, Computed)

  maintenance_window block "list" (Optional) {
    preference = string (Required)
    days_of_week = ['set', ['object', {'name': 'string'}]] (Optional)
    hours_of_day = ['set', 'number'] (Optional)
    lead_time_in_weeks = number (Optional)
    months = ['set', ['object', {'name': 'string'}]] (Optional)
    weeks_of_month = ['set', 'number'] (Optional)
  }

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
