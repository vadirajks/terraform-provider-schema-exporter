resource "aws_odb_network" "name" {
  availability_zone_id = string (Required)
  backup_subnet_cidr = string (Required)
  client_subnet_cidr = string (Required)
  display_name = string (Required)
  s3_access = string (Required)
  zero_etl_access = string (Required)
  arn = string (Computed)
  availability_zone = string (Optional, Computed)
  created_at = string (Computed)
  custom_domain_name = string (Optional)
  default_dns_prefix = string (Optional)
  delete_associated_resources = bool (Optional, Computed)
  id = string (Computed)
  managed_services = ['list', ['object', {'managed_s3_backup_access': ['list', ['object', {'ipv4_addresses': ['set', 'string'], 'status': 'string'}]], 'managed_service_ipv4_cidrs': ['set', 'string'], 'resource_gateway_arn': 'string', 's3_access': ['list', ['object', {'domain_name': 'string', 'ipv4_addresses': ['set', 'string'], 's3_policy_document': 'string', 'status': 'string'}]], 'service_network_arn': 'string', 'service_network_endpoint': ['list', ['object', {'vpc_endpoint_id': 'string', 'vpc_endpoint_type': 'string'}]], 'zero_etl_access': ['list', ['object', {'cidr': 'string', 'status': 'string'}]]}]] (Computed)
  oci_dns_forwarding_configs = ['list', ['object', {'domain_name': 'string', 'oci_dns_listener_ip': 'string'}]] (Computed)
  oci_network_anchor_id = string (Computed)
  oci_network_anchor_url = string (Computed)
  oci_resource_anchor_name = string (Computed)
  oci_vcn_id = string (Computed)
  oci_vcn_url = string (Computed)
  peered_cidrs = ['set', 'string'] (Computed)
  percent_progress = number (Computed)
  region = string (Optional, Computed)
  s3_policy_document = string (Optional)
  status = string (Computed)
  status_reason = string (Computed)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Computed)

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
