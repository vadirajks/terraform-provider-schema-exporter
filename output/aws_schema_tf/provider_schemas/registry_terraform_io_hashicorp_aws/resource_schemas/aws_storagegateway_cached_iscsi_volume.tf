resource "aws_storagegateway_cached_iscsi_volume" "name" {
  gateway_arn = string (Required)
  network_interface_id = string (Required)
  target_name = string (Required)
  volume_size_in_bytes = number (Required)
  arn = string (Computed)
  chap_enabled = bool (Computed)
  id = string (Optional, Computed)
  kms_encrypted = bool (Optional)
  kms_key = string (Optional)
  lun_number = number (Computed)
  network_interface_port = number (Computed)
  region = string (Optional, Computed)
  snapshot_id = string (Optional)
  source_volume_arn = string (Optional)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Optional, Computed)
  target_arn = string (Computed)
  volume_arn = string (Computed)
  volume_id = string (Computed)
}
