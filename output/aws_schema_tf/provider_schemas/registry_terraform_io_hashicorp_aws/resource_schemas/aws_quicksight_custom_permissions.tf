resource "aws_quicksight_custom_permissions" "name" {
  custom_permissions_name = string (Required)
  arn = string (Computed)
  aws_account_id = string (Optional, Computed)
  region = string (Optional, Computed)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Computed)

  capabilities block "list" (Optional) {
    add_or_run_anomaly_detection_for_analyses = string (Optional)
    create_and_update_dashboard_email_reports = string (Optional)
    create_and_update_data_sources = string (Optional)
    create_and_update_datasets = string (Optional)
    create_and_update_themes = string (Optional)
    create_and_update_threshold_alerts = string (Optional)
    create_shared_folders = string (Optional)
    create_spice_dataset = string (Optional)
    export_to_csv = string (Optional)
    export_to_csv_in_scheduled_reports = string (Optional)
    export_to_excel = string (Optional)
    export_to_excel_in_scheduled_reports = string (Optional)
    export_to_pdf = string (Optional)
    export_to_pdf_in_scheduled_reports = string (Optional)
    include_content_in_scheduled_reports_email = string (Optional)
    print_reports = string (Optional)
    rename_shared_folders = string (Optional)
    share_analyses = string (Optional)
    share_dashboards = string (Optional)
    share_data_sources = string (Optional)
    share_datasets = string (Optional)
    subscribe_dashboard_email_reports = string (Optional)
    view_account_spice_capacity = string (Optional)
  }
}
