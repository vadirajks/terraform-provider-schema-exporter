resource "aws_sns_topic_subscription" "name" {
  endpoint = string (Required)
  protocol = string (Required)
  topic_arn = string (Required)
  arn = string (Computed)
  confirmation_timeout_in_minutes = number (Optional)
  confirmation_was_authenticated = bool (Computed)
  delivery_policy = string (Optional)
  endpoint_auto_confirms = bool (Optional)
  filter_policy = string (Optional)
  filter_policy_scope = string (Optional, Computed)
  id = string (Optional, Computed)
  owner_id = string (Computed)
  pending_confirmation = bool (Computed)
  raw_message_delivery = bool (Optional)
  redrive_policy = string (Optional)
  region = string (Optional, Computed)
  replay_policy = string (Optional)
  subscription_role_arn = string (Optional)
}
