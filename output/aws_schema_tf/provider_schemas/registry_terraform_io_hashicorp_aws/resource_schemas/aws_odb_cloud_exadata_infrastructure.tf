resource "aws_odb_cloud_exadata_infrastructure" "name" {
  availability_zone_id = string (Required)
  display_name = string (Required)
  shape = string (Required)
  activated_storage_count = number (Computed)
  additional_storage_count = number (Computed)
  arn = string (Computed)
  availability_zone = string (Optional, Computed)
  available_storage_size_in_gbs = number (Computed)
  compute_count = number (Optional, Computed)
  compute_model = string (Computed)
  cpu_count = number (Computed)
  created_at = string (Computed)
  customer_contacts_to_send_to_oci = ['set', ['object', {'email': 'string'}]] (Optional)
  data_storage_size_in_tbs = number (Computed)
  database_server_type = string (Optional)
  db_node_storage_size_in_gbs = number (Computed)
  db_server_version = string (Computed)
  id = string (Computed)
  last_maintenance_run_id = string (Computed)
  max_cpu_count = number (Computed)
  max_data_storage_in_tbs = number (Computed)
  max_db_node_storage_size_in_gbs = number (Computed)
  max_memory_in_gbs = number (Computed)
  memory_size_in_gbs = number (Computed)
  monthly_db_server_version = string (Computed)
  monthly_storage_server_version = string (Computed)
  next_maintenance_run_id = string (Computed)
  oci_resource_anchor_name = string (Computed)
  oci_url = string (Computed)
  ocid = string (Computed)
  percent_progress = number (Computed)
  region = string (Optional, Computed)
  status = string (Computed)
  status_reason = string (Computed)
  storage_count = number (Optional, Computed)
  storage_server_type = string (Optional)
  storage_server_version = string (Computed)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Computed)
  total_storage_size_in_gbs = number (Computed)

  maintenance_window block "list" (Optional) {
    custom_action_timeout_in_mins = number (Required)
    is_custom_action_timeout_enabled = bool (Required)
    patching_mode = string (Required)
    preference = string (Required)
    days_of_week = ['set', ['object', {'name': 'string'}]] (Optional, Computed)
    hours_of_day = ['set', 'number'] (Optional, Computed)
    lead_time_in_weeks = number (Optional, Computed)
    months = ['set', ['object', {'name': 'string'}]] (Optional, Computed)
    weeks_of_month = ['set', 'number'] (Optional, Computed)
  }

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
