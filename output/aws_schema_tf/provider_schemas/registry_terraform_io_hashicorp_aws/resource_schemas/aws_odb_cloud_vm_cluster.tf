resource "aws_odb_cloud_vm_cluster" "name" {
  cloud_exadata_infrastructure_id = string (Required)
  cpu_core_count = number (Required)
  data_storage_size_in_tbs = number (Required)
  db_servers = ['set', 'string'] (Required)
  display_name = string (Required)
  gi_version = string (Required)
  hostname_prefix = string (Required)
  odb_network_id = string (Required)
  ssh_public_keys = ['set', 'string'] (Required)
  arn = string (Computed)
  cluster_name = string (Optional, Computed)
  compute_model = string (Computed)
  created_at = string (Computed)
  db_node_storage_size_in_gbs = number (Optional, Computed)
  disk_redundancy = string (Computed)
  domain = string (Computed)
  gi_version_computed = string (Computed)
  hostname_prefix_computed = string (Computed)
  id = string (Computed)
  iorm_config_cache = ['list', ['object', {'db_plans': ['list', ['object', {'db_name': 'string', 'flash_cache_limit': 'string', 'share': 'number'}]], 'lifecycle_details': 'string', 'lifecycle_state': 'string', 'objective': 'string'}]] (Computed)
  is_local_backup_enabled = bool (Optional, Computed)
  is_sparse_diskgroup_enabled = bool (Optional, Computed)
  last_update_history_entry_id = string (Computed)
  license_model = string (Optional, Computed)
  listener_port = number (Computed)
  memory_size_in_gbs = number (Optional, Computed)
  node_count = number (Computed)
  oci_resource_anchor_name = string (Computed)
  oci_url = string (Computed)
  ocid = string (Computed)
  percent_progress = number (Computed)
  region = string (Optional, Computed)
  scan_dns_name = string (Computed)
  scan_dns_record_id = string (Computed)
  scan_ip_ids = ['list', 'string'] (Computed)
  scan_listener_port_tcp = number (Optional, Computed)
  shape = string (Computed)
  status = string (Computed)
  status_reason = string (Computed)
  storage_size_in_gbs = number (Computed)
  system_version = string (Computed)
  tags = ['map', 'string'] (Optional)
  tags_all = ['map', 'string'] (Computed)
  timezone = string (Optional, Computed)
  vip_ids = ['list', 'string'] (Computed)

  data_collection_options block "list" (Optional) {
    is_diagnostics_events_enabled = bool (Required)
    is_health_monitoring_enabled = bool (Required)
    is_incident_logs_enabled = bool (Required)
  }

  timeouts block "single" (Optional) {
    create = string (Optional)
    delete = string (Optional)
    update = string (Optional)
  }
}
